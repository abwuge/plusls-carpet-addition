{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "icon": "assets/${mod_id}/icon.png",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    {
      "name": "plusls",
      "contact": {
        "homepage": "https://github.com/plusls"
      }
    }
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "issues": "${mod_sources}/issues",
    "sources": "${mod_sources}"
  },
  "license": "CC0-1.0",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.plusls.carpet.PluslsCarpetAddition"
    ],
    "server": [
      "com.plusls.carpet.PluslsCarpetAddition"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "accessWidener": "${mod_id}.accesswidener",
  "depends": {
    "carpet": "*",
    "fabric": "*",
    "magiclib": ">=${magiclib_dependency}",
    "minecraft": ">=1.21 - <1.21.2-"
  },
  "custom": {
    "modmenu:parent": "${mod_id}",
    "modmenu": {
      "parent": "${mod_id}"
    }
  }
}
